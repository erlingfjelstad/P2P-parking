<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/containerToolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include  layout="@layout/layout_toolbar"/>
    </FrameLayout>
    <eu.vincinity2020.p2p_parking.utils.compoundviews.ProfileRow
        android:id="@+id/profile_row_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/form_field_margin"
        app:imageStart="@drawable/ic_person_black_24dp"
        app:text="@string/profile" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginBottom="4dp"
        android:layout_marginTop="4dp"
        android:background="?android:attr/listDivider" />

    <eu.vincinity2020.p2p_parking.utils.compoundviews.ProfileRow
        android:id="@+id/profile_row_my_cars"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:imageStart="@drawable/ic_directions_car_black_24dp"
        app:text="@string/my_cars" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginBottom="4dp"
        android:layout_marginTop="4dp"
        android:background="?android:attr/listDivider" />


</LinearLayout>